{
    "manifest_version": 2,
    "name": "migue-detector",
    "version": "1.0",
    "description": "Checa se um artigo do PMC tá dando migué",
    "icons": {
        "48": "icons/border-48.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*.ncbi.nlm.nih.gov/pmc/articles/*"
            ],
            "js": [
                "index.js"
            ]
        }
    ]
}